---
layout: _base-layout.ejs
title: Sophia Celeste - Artist
page_css: work.css
---

<div id="filter-button-container" class="container">
  <button class="btn active" onclick="filterSelection('illustration')">ILLUSTRATION</button>
  <button class="btn" onclick="filterSelection('interactive')">INTERACTIVES</button>
  <button class="btn" onclick="filterSelection('fine-art')">FINE ART</button>
  <button class="btn" onclick="filterSelection('all')">ALL</button>
</div>
  
<div id="project-container" class="container masonry-grid">
    <div class="masonry-grid-sizer"></div>
    <% for ( let i = 0; i < projects.length; i++ ){ %>
        <% let projectPreviewUrl = projects[i].projectPreviewUrl || "/project/" + projects[i].slug; %>
        <div class="preview-img-container masonry-grid-item filterDiv <%- projects[i].category %>">
            <div class="preview-img">
                <a href="<%= projectPreviewUrl %>">
                    <img class="preview-img-png" src="<%- projects[i].previewImgUrl %>" />
                    <!--<div class="img-overlay"></div>-->
                </a>
            </div>
        </div>
    <% } %>
</div>


<script src="/js/portfolio.js"></script>

